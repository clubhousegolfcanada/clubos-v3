# HubSpot
HUBSPOT_API_KEY=
HUBSPOT_PORTAL_ID=

# Redis (optional, for caching)
REDIS_URL=redis://localhost:6379

# Monitoring (optional)
SENTRY_DSN=
DATADOG_API_KEY=

# Feature Flags
ENABLE_CLAUDE_INTEGRATION=false
ENABLE_REAL_APIS=false
DEMO_MODE=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=json