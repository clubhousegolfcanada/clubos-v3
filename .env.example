# ClubOS V3 Environment Variables

# Database
DATABASE_URL=postgresql://clubos:clubos@localhost:5432/clubos_v3

# Backend Server
BACKEND_PORT=3001
BACKEND_URL=http://localhost:3001

# Frontend Server
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Environment
NODE_ENV=development

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRY=7d

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4-turbo-preview

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Slack
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...
SLACK_CHANNEL=#clubhouse-support

# OpenPhone (future)
OPENPHONE_API_KEY=
OPENPHONE_PHONE_NUMBER=

# NinjaOne
NINJAONE_CLIENT_ID=
NINJAONE_CLIENT_SECRET=
NINJAONE_API_URL=https://api.ninjarmm.com/v2
NINJAONE_ORG_ID=
# Device IDs
NINJAONE_BEDFORD_BAY1_ID=
NINJAONE_BEDFORD_BAY2_ID=
NINJAONE_DARTMOUTH_BAY1_ID=
NINJAONE_DARTMOUTH_BAY2_ID=
NINJAONE_DARTMOUTH_BAY3_ID=
NINJAONE_DARTMOUTH_BAY4_ID=

# BenQ Projectors
BENQ_API_PORT=4661
BENQ_PASSWORD=
# Projector IPs
BENQ_BEDFORD_BAY1_IP=
BENQ_BEDFORD_BAY2_IP=
BENQ_DARTMOUTH_BAY1_IP=
BENQ_DARTMOUTH_BAY2_IP=
BENQ_DARTMOUTH_BAY3_IP=
BENQ_DARTMOUTH_BAY4_IP=

# Ubiquiti
UBIQUITI_CONTROLLER_URL=
UBIQUITI_USERNAME=
UBIQUITI_PASSWORD=
